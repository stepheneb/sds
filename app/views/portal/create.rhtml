<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN"
        "http://www.w3.org/TR/html4/strict.dtd">
<html lang="en">
  <% pagetitle = "New Portal" %>
    <head>
        <title><%= pagetitle %></title>
    </head>
<h2><%= pagetitle %></h2>

<%= link_to 'List', :action => 'list' %>

<% form_for :portal, @portal, :url => { :action => "create" } do |f| %>
  <%= render :partial => 'form' %>
  <%= f.submit "Create" %>
<% end %>

